class Solution {
public:
    bool isAnagram(string s, string t) 
    {
        unordered_map<char,int>mp1;
        for(int i=0;i<s.size();i++)
        {
            mp1[s[i]]++;
        }
        unordered_map<char,int>mp2;
        for(int i=0;i<t.size();i++)
        {
            mp2[t[i]]++;
        }
        if(mp1.size()!=mp2.size())
            return false;
        for(int i=0;i<s.size();i++)
        {
            if(mp1[s[i]]!=mp2[s[i]])
                return false;
        }
        return true;
    }
};
